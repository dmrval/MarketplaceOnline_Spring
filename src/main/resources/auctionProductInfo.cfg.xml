<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.epam.dmrval.entity.AuctionProductInfo" table="AuctionProductInfo">
        <id name="id" column="INFOID">
            <generator class="identity"/>
        </id>
        <property name="startPrice" column="STARTPRICE" type="java.lang.Double"/>
        <property name="stepLevel" column="STEPLEVEL" type="java.lang.Double"/>
    </class>
</hibernate-mapping>